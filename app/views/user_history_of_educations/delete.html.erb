<div class="row v-center-align">
  <h2 class="subheader text-center mainheader">Delete Education?</h2>
  <%= form_for(@education, :url => delete_user_user_history_of_education_path(@user), :method => 'delete') do |edu| %>
    <p class="text-center">Are you sure you want to delete this Education permanently?</p>
    <%= edu.submit("Delete Education", :class => "alert button button_dropshadow button-center") %>
  <% end %>
</div>
