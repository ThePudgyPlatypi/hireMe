<div class="row content-expand job-board">
  <div class="columns medium-12">
    <h3 class="subheader text-center mainheader">Active Job Listings</h3>
    <% @jobs.each do |job| %>
      <div class="row job orange-row">
        <div class="columns small-3 pic-col">
          <%= render(:partial => 'view', :locals => {:job => job}) %>
        <div class="columns small-3 title-col">
          <img src="http://placehold.it/350x150">
        </div>
        <div class="columns small-3 aag-col">
          <img src="http://placehold.it/350x150">
        </div>
        <div class="columns small-3 match-col">
          <img src="http://placehold.it/350x150">
        </div>
      </div>
      </div>
    <% end %>
  </div>
</div>


